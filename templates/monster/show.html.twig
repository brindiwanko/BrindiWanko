{% extends 'base.html.twig' %}

{% block title %}Monster{% endblock %}

{% block body %}
    <h1>Monster</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ monster.id }}</td>
            </tr>
            <tr>
                <th>Picture</th>
                <td>{{ monster.picture }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ monster.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ monster.description }}</td>
            </tr>
            <tr>
                <th>Level</th>
                <td>{{ monster.level }}</td>
            </tr>
            <tr>
                <th>Hp</th>
                <td>{{ monster.hp }}</td>
            </tr>
            <tr>
                <th>Mp</th>
                <td>{{ monster.mp }}</td>
            </tr>
            <tr>
                <th>Strength</th>
                <td>{{ monster.strength }}</td>
            </tr>
            <tr>
                <th>Magic</th>
                <td>{{ monster.magic }}</td>
            </tr>
            <tr>
                <th>Agility</th>
                <td>{{ monster.agility }}</td>
            </tr>
            <tr>
                <th>Defense</th>
