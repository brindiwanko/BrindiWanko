            </tr>
            <tr>
                <th>Quantity</th>
                <td>{{ monster.quantity }}</td>
            </tr>
            <tr>
                <th>QuantityBattle</th>
                <td>{{ monster.quantityBattle }}</td>
            </tr>
            <tr>
                <th>QuantityEscaped</th>
                <td>{{ monster.quantityEscaped }}</td>
            </tr>
            <tr>
                <th>QuantityVictory</th>
                <td>{{ monster.quantityVictory }}</td>
            </tr>
            <tr>
                <th>QuantityDefeated</th>
                <td>{{ monster.quantityDefeated }}</td>
            </tr>
            <tr>
