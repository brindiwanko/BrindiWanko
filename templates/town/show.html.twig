{% extends 'base.html.twig' %}

{% block title %}Town{% endblock %}

{% block body %}
    <h1>Town</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ town.id }}</td>
            </tr>
            <tr>
                <th>Picture</th>
                <td>{{ town.picture }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ town.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ town.description }}</td>
            </tr>
            <tr>
                <th>PriceInn</th>
                <td>{{ town.priceInn }}</td>
            </tr>
            <tr>
                <th>Chapter</th>
                <td>{{ town.chapter }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_town_index') }}">back to list</a>

    <a href="{{ path('app_town_edit', {'id': town.id}) }}">edit</a>

    {{ include('town/_delete_form.html.twig') }}
{% endblock %}
