{% extends 'base.html.twig' %}

{% block title %}Job{% endblock %}

{% block body %}
    <h1>Job</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ job.id }}</td>
            </tr>
            <tr>
                <th>Picture</th>
                <td>{{ job.picture }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ job.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ job.description }}</td>
            </tr>
            <tr>
                <th>HpBonus</th>
                <td>{{ job.hpBonus }}</td>
            </tr>
            <tr>
                <th>MpBonus</th>
                <td>{{ job.mpBonus }}</td>
            </tr>
            <tr>
                <th>StrengthBonus</th>
                <td>{{ job.strengthBonus }}</td>
            </tr>
            <tr>
                <th>MagicBonus</th>
                <td>{{ job.magicBonus }}</td>
            </tr>
            <tr>
                <th>AgilityBonus</th>
                <td>{{ job.agilityBonus }}</td>
            </tr>
            <tr>
                <th>DefenseBonus</th>
                <td>{{ job.defenseBonus }}</td>
            </tr>
            <tr>
                <th>DefenseMagicBonus</th>
                <td>{{ job.defenseMagicBonus }}</td>
            </tr>
            <tr>
                <th>WisdomBonus</th>
                <td>{{ job.wisdomBonus }}</td>
            </tr>
            <tr>
                <th>ProspectingBonus</th>
                <td>{{ job.prospectingBonus }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_job_index') }}">back to list</a>

    <a href="{{ path('app_job_edit', {'id': job.id}) }}">edit</a>

    {{ include('job/_delete_form.html.twig') }}
{% endblock %}
